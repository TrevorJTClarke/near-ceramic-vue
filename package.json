{
  "name": "near-vue-tailwind",
  "version": "0.1.1",
  "description": "NEAR Protocol starter kit including Vue 3 & tailwind 2, enabling easy setup for a blockchain dApp",
  "author": "trevorjtclarke",
  "scripts": {
    "dev": "webpack-dev-server",
    "devv": "vite --open",
    "build": "cross-env NODE_ENV=production vite build",
    "prestart": "npm run contract:build && npm run contract:dev:deploy",
    "contract:build": "node ./contract/build",
    "contract:dev:deploy": "near dev-deploy --wasmFile ./contract/res/near_ceramic.wasm",
    "contract:deploy": "near deploy --wasmFile ./contract/res/near_ceramic.wasm",
    "contract:test": "cd contract && cargo test --package status-message -- --nocapture"
  },
  "keywords": [
    "NEAR",
    "NEAR Protocol",
    "NEAR",
    "Blockchain",
    "NEAR Blockchain",
    "Vue",
    "Vuejs",
    "Vue 3",
    "Tailwind"
  ],
  "dependencies": {
    "@ceramicnetwork/http-client": "^0.9.5",
    "luxon": "^1.25.0",
    "vue": "^3.0.4",
    "vue-axios": "^3.2.1",
    "vue-near": "^0.2.1",
    "vue-router": "^4.0.1",
    "vuex": "^4.0.0-rc.2"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.2.0",
    "@tailwindcss/forms": "^0.2.1",
    "@tailwindcss/typography": "^0.3.1",
    "@vitejs/plugin-vue": "^1.0.4",
    "@vue/compiler-sfc": "^3.0.5",
    "@webpack-cli/serve": "^1.3.0",
    "autoprefixer": "^9.8.6",
    "cross-env": "^7.0.3",
    "css-loader": "^5.0.2",
    "file-loader": "^6.2.0",
    "mini-css-extract-plugin": "^1.3.6",
    "near-api-js": "^0.36.2",
    "postcss": "^7.0.35",
    "shelljs": "^0.8.4",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.2",
    "url-loader": "^4.1.1",
    "vite": "^2.0.0-beta.8",
    "vue-loader": "^16.0.0-beta.8",
    "vue-template-compiler": "2.6.10",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
